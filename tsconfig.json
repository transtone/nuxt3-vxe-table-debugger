    "noImplicitAny": false
  },
  "vueCompilerOptions": {
    "plugins": [
      "@vue-macros/volar/short-vmodel"
    ],
    "vueMacros": {
      "shortVmodel": {
        "prefix": "$"
      }
    }
  },
  "exclude": [ "./dist", "./.vscode", "./node_modules", "./.output", "./.yarn", "./assets" ],
  "extends": "./.nuxt/tsconfig.json",
}